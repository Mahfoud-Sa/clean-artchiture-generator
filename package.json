{
  "name": "clean-architecture-generator",
  "displayName": "Clean Architecture Generator",
  "description": "Generates Clean Architecture structure for Flutter projects",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:clean-architecture-generator.helloWorld",
    "onCommand:clean-architecture-generator.initCleanProject"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clean-architecture-generator.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "clean-architecture-generator.initCleanProject",
        "title": "Initialize Clean Architecture Project"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}